{
  "name": "py-manager",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Suvansh Rana",
    "email": "suvansh.rana.dev@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suvansh-rana/py_manager.git"
  },
  "dependencies": {
    "@hookform/resolvers": "^1.3.4",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "compare-versions": "^3.6.0",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "electron-store": "^7.0.2",
    "electron-updater": "^4.3.9",
    "marked": "^2.0.3",
    "node-cache": "^5.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.1",
    "react-icons": "^4.1.0",
    "react-lazyload": "^3.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "rst2html": "^1.0.4",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.8"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-react-start": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .  \"",
    "electron:build": "npm run build && electron-builder -c.extraMetadata.main=build/electron.js",
    "electron:publish": "npm run build && electron-builder -c.extraMetadata.main=build/electron.js --publish onTag"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.1.1",
    "wait-on": "^5.2.1"
  },
  "build": {
    "extends": null,
    "appId": "com.suvansh-rana.${name}",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "productName": "Py Manager",
    "publish": [
      {
        "provider": "github",
        "owner": "suvansh-rana",
        "repo": "py_manager",
        "private": true
      }
    ],
    "linux": {
      "publish": [
        "github"
      ],
      "target": [
        {
          "target": "deb"
        }
      ]
    },
    "win": {
      "publish": [
        "github"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "homepage": "./"
}